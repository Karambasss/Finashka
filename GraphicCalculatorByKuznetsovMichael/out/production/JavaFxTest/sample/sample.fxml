<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="328.0" prefWidth="509.0" spacing="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <padding>
        <Insets bottom="5" left="20" right="5" top="10" />
    </padding>
    <Label fx:id="lbl_control" text="Калькулятор" underline="true" />
   <TextField fx:id="textField" />
   <TextField fx:id="textField2" />
   <HBox prefHeight="127.0" prefWidth="484.0" spacing="10">
       <children>
         <Button fx:id="plus" mnemonicParsing="false" onAction="#plus_click" text="+" />
         <Button fx:id="minus" mnemonicParsing="false" onAction="#minus_click" text="-" />
         <Button fx:id="umnozhit" mnemonicParsing="false" onAction="#umnozhit_click" text="*" />
         <Button fx:id="delenie" mnemonicParsing="false" onAction="#delenie_click" text="/" />
      </children>
   </HBox>
    <opaqueInsets>
        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
    </opaqueInsets>


</VBox>
